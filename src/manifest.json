	"version": "0.1",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"*://ta.yrdsb.ca/live/students/viewReport.php?*"
			],
			"js": ["js/ta.js"]
		}
	],
	"permissions": ["storage"]
}	
